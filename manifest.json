{
  "manifest_version": 2,

  "name": "bookmarks rss",
  "description": "书签-RSS",
  "version": "1.0",
  "icons": {
    "24": "rss-icon.png",
    "38": "rss-icon1.png",
    "128": "rss-icon2.png"
  },
  "browser_action": {
    "default_icon": "rss-icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "identity",
    "identity.email",
    "bookmarks",
    "http://localhost:3000/"
  ],
  "content_security_policy": "script-src 'self' http://localhost:3000/; object-src 'self'"
}